   "source": [
    "f = lambda x : x**4 + 2*(x**3) - x - 1\n",
    "left = 0\n",
    "right = 10\n",
    "e = 0.001\n",
    "while right - left > e:\n",
    "    mid = (left + right) / 2\n",
    "    if f(left)*f(mid) < 0:\n",
    "        right = mid\n",
    "    else:\n",
    "        left = mid\n",
    "print(left)"
   ]
  }
